<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>nexus-repo</id>
      <username>INJECTED-USERNAME</username>
      <password>INJECTED-PASSWORD</password>
    </server>
    <server>
      <id>nexus-repo-snapshot</id>
      <username>INJECTED-USERNAME</username>
      <password>INJECTED-PASSWORD</password>
    </server>
    <server>
      <id>nexus-pull-repo</id>
      <username>INJECTED-USERNAME</username>
      <password>INJECTED-PASSWORD</password>
    </server>
  </servers>
  <!-- Nexus repo to pull from -->
  <activeProfiles>
    <activeProfile>my-nexus-repo</activeProfile>
  </activeProfiles>
</settings>